{
  "name": "todo-api",
  "version": "1.0.0",
  "type": "module",
  "description": "REST API for managing todos with authentication",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
